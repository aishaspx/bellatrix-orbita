{
    "version": 2,
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/backend/main.py"
        },
        {
            "source": "/style.css",
            "destination": "/frontend/style.css"
        },
        {
            "source": "/(.*)",
            "destination": "/frontend/index.html"
        }
    ]
}